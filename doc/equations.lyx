\begin_layout Subsection
Neutral fluids
\end_layout

\begin_layout Standard
We need expressions for 
\begin_inset Formula $\Pi$
\end_inset

 and 
\begin_inset Formula $\boldsymbol{h}$
\end_inset

 in order to close the system of equations.
 Assuming an isotropic viscosity for unmagnetized flow we may apply the
 following for the viscous stress tensor,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Pi & =-\mu\left(\nabla\boldsymbol{u}+\left(\nabla\boldsymbol{u}\right)^{T}\right)-\lambda\mathbf{I}\nabla\cdot\boldsymbol{u},\\
\Pi & =-\left(\begin{array}{ccc}
2\mu\frac{\partial u}{\partial x}+\lambda\left(\frac{\partial u}{\partial x}+\frac{\partial v}{\partial y}+\frac{\partial w}{\partial z}\right) & \mu\left(\frac{\partial u}{\partial y}+\frac{\partial v}{\partial x}\right) & \mu\left(\frac{\partial u}{\partial z}+\frac{\partial w}{\partial x}\right)\\
\mu\left(\frac{\partial u}{\partial y}+\frac{\partial v}{\partial x}\right) & 2\mu\frac{\partial v}{\partial y}+\lambda\left(\frac{\partial u}{\partial x}+\frac{\partial v}{\partial y}+\frac{\partial w}{\partial z}\right) & \mu\left(\frac{\partial v}{\partial z}+\frac{\partial w}{\partial y}\right)\\
\mu\left(\frac{\partial u}{\partial z}+\frac{\partial w}{\partial x}\right) & \mu\left(\frac{\partial v}{\partial z}+\frac{\partial w}{\partial y}\right) & 2\mu\frac{\partial w}{\partial z}+\lambda\left(\frac{\partial u}{\partial x}+\frac{\partial v}{\partial y}+\frac{\partial w}{\partial z}\right)
\end{array}\right),
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\mu$
\end_inset

 is the viscosity and 
\begin_inset Formula $\lambda=-\frac{2}{3}\mu$
\end_inset

 is the bulk viscosity according to Stokes hypothesis.
 We may use a constant viscosity or any of the temperature dependent relationshi
ps such as a power law model or Sutherlands equation.
\end_layout

\begin_layout Standard
The heat flux is given by, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\boldsymbol{h} & =-\kappa\nabla T,\\
\kappa & =c_{p}\frac{\mu}{\mathrm{Pr}}.
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Magnetised fluids (NOT IMPLEMENTED)
\end_layout

\begin_layout Standard
For a magnetised fluid we have 
\begin_inset CommandInset citation
LatexCommand cite
key "Krommes2018,Braginskii1965"
literal "false"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Pi & =-nm\boldsymbol{m}\colon\boldsymbol{S}\\
\Pi & =-nm\sum_{p=0}^{4}n\mu_{p}\boldsymbol{V}_{p}\boldsymbol{\colon}\boldsymbol{S}\\
\boldsymbol{V}_{0}\colon\boldsymbol{S} & =\frac{3}{2}\left(\boldsymbol{B}-\frac{1}{3}\boldsymbol{I}\right)\left(\boldsymbol{B}-\frac{1}{3}\boldsymbol{I}\right)\boldsymbol{\colon}\boldsymbol{W}\\
\boldsymbol{V}_{1}\colon\boldsymbol{S} & =\boldsymbol{\delta}^{\perp}\cdot\boldsymbol{W}.\boldsymbol{\delta}^{\perp}+\frac{1}{2}\left(\hat{\boldsymbol{b}}\cdot\boldsymbol{W}.\hat{\boldsymbol{b}}\right)\boldsymbol{\delta}^{\perp}\\
\boldsymbol{V}_{2}\colon\boldsymbol{S} & =\boldsymbol{\delta}^{\perp}\cdot\boldsymbol{W}.\boldsymbol{B}+\boldsymbol{B}\cdot\boldsymbol{W}.\boldsymbol{\delta}^{\perp}\\
\boldsymbol{V}_{3}\colon\boldsymbol{S} & =\frac{1}{2}\left(\boldsymbol{\delta}^{\perp}\cdot\boldsymbol{W}.\boldsymbol{\beta}-\boldsymbol{\beta}\cdot\boldsymbol{W}.\boldsymbol{\delta}^{\perp}\right)\\
\boldsymbol{V}_{4}\colon\boldsymbol{S} & =\boldsymbol{B}\cdot\boldsymbol{W}.\boldsymbol{\beta}-\boldsymbol{\beta}\cdot\boldsymbol{W}.\boldsymbol{B}\\
\boldsymbol{W} & =2\left(\boldsymbol{S}-\frac{1}{3}\nabla\cdot\boldsymbol{u}\boldsymbol{I}\right)\\
\boldsymbol{S} & =\frac{1}{2}\left[\nabla\boldsymbol{u}+\left(\nabla\boldsymbol{u}\right)^{T}\right]\\
\boldsymbol{B} & =\hat{\boldsymbol{b}}\hat{\boldsymbol{b}}=b_{i}b_{j}\\
\boldsymbol{\delta}^{\perp} & =\boldsymbol{I}-\boldsymbol{B}=\delta_{ij}^{\perp}\\
\boldsymbol{\beta} & =-\epsilon_{ijk}\cdot b_{j}\\
\hat{\boldsymbol{b}} & =\nicefrac{\boldsymbol{B}}{\left|\boldsymbol{B}\right|}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
where (cgs units, quasi-neutral, 
\begin_inset Formula $\nicefrac{m_{e}}{m_{i}}\ll1$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mu_{0}^{i} & =0.96n_{i}T_{i}\tau_{i}, & \mu_{0}^{e} & =0.733n_{e}T_{e}\tau_{e},\\
\mu_{2}^{i} & =\frac{n_{i}T_{i}\tau_{i}}{\Delta}\left(\frac{6}{5}x^{2}+2.23\right) & \mu_{2}^{e} & =\frac{n_{e}T_{e}\tau_{e}}{\Delta}\left(2.05x^{2}+8.5\right)\\
\mu_{4}^{i} & =\frac{n_{i}T_{i}\tau_{i}x}{\Delta}\left(x^{2}+2.38\right) & \mu_{4}^{e} & =-\frac{n_{e}T_{e}\tau_{e}x}{\Delta}\left(x^{2}+7.91\right)\\
\mu_{1}^{i} & =\mu_{2}^{i}\left(2x\right) & \mu_{1}^{e} & =\mu_{2}^{e}\left(2x\right)\\
\mu_{3}^{i} & =\mu_{4}^{i}\left(2x\right) & \mu_{3}^{e} & =\mu_{4}^{e}\left(2x\right)\\
x & =\omega_{i}\tau_{i} & x & =\omega_{e}\tau_{e}\\
\Delta & =x^{4}+4.03x^{2}+2.33 & \Delta & =x^{4}+13.8x^{2}+11.6\\
\omega_{i} & =\frac{ZeB}{m_{i}c} & \omega_{e} & =\frac{eB}{m_{e}c}\\
\begin{tabular}{c}
 \tau_{i}\end{tabular} & =\frac{3\sqrt{m_{i}}T_{i}^{\nicefrac{3}{2}}}{4\sqrt{\pi}\lambda e^{4}Z^{4}n_{i}} & \begin{tabular}{c}
 \tau_{e}\end{tabular} & =\frac{3\sqrt{m_{e}}T_{e}^{\nicefrac{3}{2}}}{4\sqrt{2\pi}\lambda e^{4}Z^{2}n_{i}}\\
\lambda & =\begin{cases}
23.4-1.15\log n+3.45\log T_{e}, & T_{e}<50eV,\\
25.3-1.15\log n+2.3\log T_{e}, & T_{e}>50eV
\end{cases}
\end{align*}

\end_inset


\end_layout

