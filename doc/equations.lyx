\frac{B_{z}^{*R}}{\mu_{R}}= & \frac{B_{z}^{R}}{\mu_{R}}-\frac{1}{\varepsilon_{0}c_{0}c_{R}\mu_{R}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}+\varepsilon_{0}c_{0}c_{L}\mu_{L}\left(\frac{B_{z}^{*L}}{\mu_{L}}-\frac{B_{z}^{L}}{\mu_{L}}\right)\right)\\
\frac{B_{z}^{*R}}{\mu_{R}}= & \frac{B_{z}^{R}}{\mu_{R}}-\frac{1}{\varepsilon_{0}c_{0}c_{R}\mu_{R}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)-\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\left(\frac{B_{z}^{*L}}{\mu_{L}}-\frac{B_{z}^{L}}{\mu_{L}}\right)\\
\frac{B_{z}^{*R}}{\mu_{R}}+\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\frac{B_{z}^{*L}}{\mu_{L}}= & \frac{B_{z}^{R}}{\mu_{R}}+\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\frac{B_{z}^{L}}{\mu_{L}}-\frac{1}{\varepsilon_{0}c_{0}c_{R}\mu_{R}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\\
\frac{B_{z}^{*R}}{\mu_{R}}\left(1+\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\right)= & \frac{B_{z}^{R}}{\mu_{R}}+\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\frac{B_{z}^{L}}{\mu_{L}}-\frac{1}{\varepsilon_{0}c_{0}c_{R}\mu_{R}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\\
\left(1+\frac{c_{L}\mu_{L}}{c_{R}\mu_{R}}\right)^{-1}= & \frac{c_{R}\mu_{R}}{c_{R}\mu_{R}+c_{L}\mu_{L}}\\
\frac{B_{z}^{*R}}{\mu_{R}}= & \frac{c_{R}}{c_{R}\mu_{R}+c_{L}\mu_{L}}B_{z}^{R}+\frac{c_{L}}{c_{R}\mu_{R}+c_{L}\mu_{L}}B_{z}^{L}-\frac{1}{\varepsilon_{0}c_{0}}\frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\\
\frac{B_{z}^{*R}}{\mu_{R}}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(c_{R}B_{z}^{R}+c_{L}B_{z}^{L}-\frac{1}{\varepsilon_{0}c_{0}}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\right)\\
\\
F_{D_{y}}= & \frac{B_{z}^{*R}}{\mu_{0}\mu_{R}}\\
F_{D_{y}}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\frac{1}{\mu_{0}}\left(c_{R}B_{z}^{R}+c_{L}B_{z}^{L}\right)-c_{0}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Non-dimensionalisation (note that 
\begin_inset Formula $c_{r}^{2}=\frac{1}{\varepsilon_{r}\mu_{r}}$
\end_inset

 with 
\begin_inset Formula $\varepsilon_{r}$
\end_inset

 and 
\begin_inset Formula $\mu_{r}$
\end_inset

 already nondimensional):
\end_layout

\begin_layout Standard
Reference quantities 
\begin_inset Formula $D_{0}=\varepsilon_{0}E_{0}$
\end_inset

, 
\begin_inset Formula $B_{0}=\mu_{0}H_{0}$
\end_inset

, 
\begin_inset Formula $E_{0}=c_{0}B_{0}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $D_{0}=\nicefrac{H_{0}}{c_{0}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\psi_{D}=\hat{\psi}_{D}H_{0}$
\end_inset

, 
\begin_inset Formula $\psi_{B}=\hat{\psi}_{B}E_{0}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
F_{D}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\frac{s}{\mu_{0}}\left(c_{R}B_{z}^{R}+c_{L}B_{z}^{L}\right)-c_{0}\left(\frac{D_{y}^{R}}{\varepsilon_{R}}-\frac{D_{y}^{L}}{\varepsilon_{L}}\right)\right)\\
F_{D}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\left[\frac{B_{0}}{\mu_{0}}\right]s\left(c_{R}\hat{B}_{z}^{R}+c_{L}\hat{B}_{z}^{L}\right)-\left[\frac{D_{0}c_{0}}{1}\right]\left(\frac{\hat{D}_{y}^{R}}{\varepsilon_{R}}-\frac{\hat{D}_{y}^{L}}{\varepsilon_{L}}\right)\right)\\
F_{D}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\left[\frac{B_{0}}{\mu_{0}}\right]s\left(c_{R}\hat{B}_{z}^{R}+c_{L}\hat{B}_{z}^{L}\right)-\left[\frac{D_{0}u_{0}}{1}\right]\hat{c}_{0}\left(\frac{\hat{D}_{y}^{R}}{\varepsilon_{R}}-\frac{\hat{D}_{y}^{L}}{\varepsilon_{L}}\right)\right)\\
\frac{B_{0}}{\mu_{0}}= & \frac{\mu_{0}H_{0}}{\mu_{0}}=c_{0}D_{0}=D_{0}u_{0}\hat{c}_{0}\\
F_{D}= & \frac{1}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(\left[\frac{D_{0}u_{0}}{1}\right]s\hat{c}_{0}\left(c_{R}\hat{B}_{z}^{R}+c_{L}\hat{B}_{z}^{L}\right)-\left[\frac{D_{0}u_{0}}{1}\right]\hat{c}_{0}\left(\frac{\hat{D}_{y}^{R}}{\varepsilon_{R}}-\frac{\hat{D}_{y}^{L}}{\varepsilon_{L}}\right)\right)\\
\frac{F_{D}}{D_{0}u_{0}}=\hat{F}_{D}= & \frac{\hat{c}_{0}}{c_{R}\mu_{R}+c_{L}\mu_{L}}\left(s\left(c_{R}\hat{B}_{z}^{R}+c_{L}\hat{B}_{z}^{L}\right)+\left(\frac{\hat{D}_{y}^{L}}{\varepsilon_{L}}-\frac{\hat{D}_{y}^{R}}{\varepsilon_{R}}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Accelerations
\end_layout

\begin_layout Standard
Acceleration of any species may be recovered from the momentum equation
 according to 
\begin_inset Formula $\mathbf{a}=\frac{d\mathbf{u}}{dt}$
\end_inset

.
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\rho_{\alpha}\mathbf{u}_{\alpha}}{\partial t}+\nabla\cdot\left(\rho_{\alpha}\mathbf{u}_{\alpha}\mathbf{u}_{\alpha}+p_{\alpha}\mathbf{I}\right)= & n_{\alpha}q_{\alpha}\left(\mathbf{E}+\mathbf{u}_{\alpha}\times\mathbf{B}\right)\\
\left(\frac{\partial}{\partial t}+\mathbf{u}_{\alpha}\cdot\nabla\right)\mathbf{u}_{\alpha}+\frac{\nabla p_{\alpha}}{n_{\alpha}m_{\alpha}}= & \frac{q_{\alpha}}{m_{\alpha}}\left(\mathbf{E}+\mathbf{u}_{\alpha}\times\mathbf{B}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Now 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{d}{dt} & =\frac{\partial}{\partial t}+\mathbf{u}\cdot\nabla
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
So
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{d\mathbf{u}_{\alpha}}{dt}+\frac{\nabla p_{\alpha}}{n_{\alpha}m_{\alpha}}= & \frac{q_{\alpha}}{m_{\alpha}}\left(\mathbf{E}+\mathbf{u}_{\alpha}\times\mathbf{B}\right)\\
\mathbf{a}= & \frac{q_{\alpha}}{m_{\alpha}}\left(\mathbf{E}+\mathbf{u}_{\alpha}\times\mathbf{B}\right)-\frac{\nabla p_{\alpha}}{n_{\alpha}m_{\alpha}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\left[\frac{u_{0}^{2}}{x_{0}}\right]\hat{\mathbf{a}} & =\left[\frac{q_{0}u_{0}B_{0}}{m_{0}}\right]\hat{r}_{\alpha}\hat{c}\mathbf{E}+\left[\frac{q_{0}u_{0}B_{0}}{m_{0}}\right]\hat{r}_{\alpha}\left(\hat{\mathbf{u}}_{\alpha}\times\hat{\mathbf{B}}\right)-\left[\frac{u_{0}^{2}}{x_{0}}\right]\frac{\hat{\nabla}\hat{p}_{\alpha}}{\hat{n}_{\alpha}\hat{m}_{\alpha}}\\
\hat{\mathbf{a}} & =\left[\frac{x_{0}q_{0}B_{0}}{u_{0}m_{0}}\right]\hat{r}_{\alpha}\left(\hat{c}\mathbf{E}+\left(\hat{\mathbf{u}}_{\alpha}\times\hat{\mathbf{B}}\right)\right)-\frac{\hat{\nabla}\hat{p}_{\alpha}}{\hat{n}_{\alpha}\hat{m}_{\alpha}}\\
\hat{\mathbf{a}} & =\frac{\hat{r}_{\alpha}}{\hat{d}_{L,0}}\left(\hat{c}\mathbf{E}+\left(\hat{\mathbf{u}}_{\alpha}\times\hat{\mathbf{B}}\right)\right)-\frac{\hat{\nabla}\hat{p}_{\alpha}}{\hat{\rho}_{\alpha}}
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Vorticity equation
\end_layout

\begin_layout Standard
Starting from the non-dimensional momentum equation.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\rho_{\alpha}\mathbf{U}_{\alpha}}{\partial t}+\nabla\cdot\left(\rho_{\alpha}\mathbf{U}_{\alpha}\mathbf{U}_{\alpha}+p_{\alpha}\mathbf{I}\right) & =\frac{\rho_{\alpha}r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\\
\frac{\partial\mathbf{U}_{\alpha}}{\partial t}+\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}+\frac{1}{\rho_{\alpha}}\nabla p_{\alpha} & =\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
using the identity 
\begin_inset Formula $\left(\mathbf{A}\cdot\nabla\right)\mathbf{A}=\frac{1}{2}\nabla\left(\mathbf{A}\cdot\mathbf{A}\right)-\mathbf{A}\times\left(\nabla\times\mathbf{A}\right)$
\end_inset

 we get 
\begin_inset Formula $\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}=\frac{1}{2}\nabla\left(\mathbf{U}_{\alpha}\cdot\mathbf{U}_{\alpha}\right)-\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}$
\end_inset

 and thus
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\mathbf{U}_{\alpha}}{\partial t}+\frac{1}{2}\nabla\left(\mathbf{U}_{\alpha}\cdot\mathbf{U}_{\alpha}\right)-\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}+\frac{1}{\rho_{\alpha}}\nabla p_{\alpha} & =\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\\
\nabla\times\left(\frac{\partial\mathbf{U}_{\alpha}}{\partial t}+\frac{1}{2}\nabla\left(\mathbf{U}_{\alpha}\cdot\mathbf{U}_{\alpha}\right)-\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}+\frac{1}{\rho_{\alpha}}\nabla p_{\alpha}\right) & =\nabla\times\left(\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
now 
\begin_inset Formula $\nabla\times\nabla\phi=0$
\end_inset

 such that 
\begin_inset Formula $\nabla\times\left(\frac{1}{2}\nabla\left(\mathbf{U}_{\alpha}\cdot\mathbf{U}_{\alpha}\right)\right)=0$
\end_inset

 and we get
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}-\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)+\nabla\times\left(\frac{1}{\rho_{\alpha}}\nabla p_{\alpha}\right) & =\nabla\times\left(\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
now 
\begin_inset Formula $\nabla\times(\mathbf{A}\times\mathbf{B})=\mathbf{A}(\nabla\cdot\mathbf{B})-\mathbf{B}(\nabla\cdot\mathbf{A})+(\mathbf{B}\cdot\nabla)\mathbf{A}-(\mathbf{A}\cdot\nabla)\mathbf{B}$
\end_inset

 such that
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right) & =\mathbf{U}_{\alpha}\left(\cancelto{0}{\nabla\cdot\mathbf{\omega}_{\alpha}}\right)-\mathbf{\omega}_{\alpha}\left(\nabla\cdot\mathbf{U}_{\alpha}\right)+\left(\mathbf{\omega}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}-\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{\omega}_{\alpha}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
giving
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}+\mathbf{\omega}_{\alpha}\left(\nabla\cdot\mathbf{U}_{\alpha}\right)-\left(\mathbf{\omega}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}+\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{\omega}_{\alpha}+\nabla\times\left(\frac{1}{\rho_{\alpha}}\nabla p_{\alpha}\right)=\nabla\times\left(\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
also 
\begin_inset Formula $\nabla\times\left(\psi\nabla\phi\right)=\nabla\psi\times\nabla\phi$
\end_inset

 and 
\begin_inset Formula $\nabla\left(\frac{\psi}{\phi}\right)=\frac{\phi\nabla\psi-(\nabla\phi)\psi}{\phi^{2}}$
\end_inset

 such that
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\nabla\times\left(\frac{1}{\rho_{\alpha}}\nabla p_{\alpha}\right) & =\nabla\left(\frac{1}{\rho_{\alpha}}\right)\times\nabla p_{\alpha}\\
 & =\left(-\frac{1}{\rho_{\alpha}^{2}}\nabla\rho_{\alpha}\right)\times\nabla p_{\alpha}\\
 & =-\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
resulting in
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}+\mathbf{\omega}_{\alpha}\left(\nabla\cdot\mathbf{U}_{\alpha}\right)-\left(\mathbf{\omega}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}+\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{\omega}_{\alpha}-\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right) & =\nabla\times\left(\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)\\
 & =\nabla\times\left(\sqrt{\frac{2}{\beta}}\frac{r_{\alpha}}{d_{S}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
or 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}=\underbrace{\left(\mathbf{\omega}_{\alpha}\cdot\nabla\right)\mathbf{U}_{\alpha}}_{\tau_{v}}-\underbrace{\left(\mathbf{U}_{\alpha}\cdot\nabla\right)\mathbf{\omega}_{\alpha}}_{\tau_{c}}-\underbrace{\mathbf{\omega}_{\alpha}\left(\nabla\cdot\mathbf{U}_{\alpha}\right)}_{\tau_{s}}\\
+\underbrace{\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right)}_{\tau_{b}}+\underbrace{\sqrt{\frac{2}{\beta}}\frac{r_{\alpha}c}{d_{S}}\left(\nabla\times\mathbf{E}\right)}_{\tau_{E}}+\underbrace{\sqrt{\frac{2}{\beta}}\frac{r_{\alpha}}{d_{S}}\left(\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)}_{\tau_{B}}
\end{multline*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\tau_{v}$
\end_inset

: 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
Stretch due to velocity
\end_layout

\begin_layout Itemize
\begin_inset Formula $\tau_{c}$
\end_inset

: Convective term
\end_layout

\begin_layout Itemize
\begin_inset Formula $\tau_{s}$
\end_inset

: Stretch due to compressibility
\end_layout

\begin_layout Section
Self generation of magnetic fields
\end_layout

\begin_layout Standard
The non-dimensional Maxwell's equations (neglecting Lagrange multiplier)
 give us 
\begin_inset Formula $-\frac{1}{c}\frac{\partial\mathbf{B}}{\partial t}=\nabla\times\mathbf{E}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\hat{\mathbf{E}}}{\partial\hat{t}}-\hat{c}\hat{\nabla}\times\hat{\mathbf{B}}= & -\frac{\hat{\vartheta}_{0}}{\hat{\lambda}_{0}^{2}\hat{c}}\sum_{\alpha}\hat{\rho}_{\alpha}\hat{r}_{\alpha}\hat{\mathbf{U}}_{\alpha},\\
\frac{\partial\hat{\mathbf{B}}}{\partial\hat{t}}+\hat{c}\hat{\nabla}\times\hat{\mathbf{E}}= & 0,
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Thus 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}-\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)-\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right) & =\nabla\times\left(\frac{r_{\alpha}}{d_{L}}\left(c\mathbf{E}+\mathbf{U}_{\alpha}\times\mathbf{B}\right)\right)\\
 & =\frac{r_{\alpha}c}{d_{L}}\nabla\times\mathbf{E}+\frac{r_{\alpha}}{d_{L}}\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)\\
\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}-\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)-\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right) & =-\frac{r_{\alpha}}{d_{L}}\frac{\partial\mathbf{B}}{\partial t}+\frac{r_{\alpha}}{d_{L}}\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{r_{\alpha}}{d_{L}}\frac{\partial\mathbf{B}}{\partial t} & =\frac{1}{\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right)+\frac{r_{\alpha}}{d_{L}}\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)-\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}+\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)\\
\frac{\partial\mathbf{B}}{\partial t} & =\frac{d_{L}}{r_{\alpha}\rho_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right)+\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)-\frac{d_{L}}{r_{\alpha}}\left(\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}-\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)\right)\\
\frac{\partial\mathbf{B}}{\partial t} & =\frac{d_{L}}{q_{\alpha}m_{\alpha}n_{\alpha}^{2}}\left(\nabla\rho_{\alpha}\times\nabla p_{\alpha}\right)+\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{B}\right)-\frac{d_{L}m_{\alpha}}{q_{\alpha}}\left(\frac{\partial\mathbf{\omega}_{\alpha}}{\partial t}-\nabla\times\left(\mathbf{U}_{\alpha}\times\mathbf{\omega}_{\alpha}\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Backwards Euler
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
f\left(\boldsymbol{u}^{n+1}\right) & =\frac{\boldsymbol{u}^{n+1}-\boldsymbol{u}^{n}}{h}\\
f\left(\boldsymbol{u}^{n+1}\right) & =f\left(\boldsymbol{u}^{n}\right)+\frac{\partial f}{\partial\boldsymbol{u}^{n}}\left(\boldsymbol{u}^{n+1}-\boldsymbol{u}^{n}\right)\\
f\left(\boldsymbol{u}^{n+1}\right) & =f\left(\boldsymbol{u}^{n}\right)+h\frac{\partial f}{\partial\boldsymbol{u}^{n}}f\left(\boldsymbol{u}^{n+1}\right)\\
f\left(\boldsymbol{u}^{n}\right) & =f\left(\boldsymbol{u}^{n+1}\right)-h\frac{\partial f}{\partial\boldsymbol{u}^{n}}f\left(\boldsymbol{u}^{n+1}\right)\\
f\left(\boldsymbol{u}^{n}\right) & =\left(\boldsymbol{I}-h\frac{\partial f}{\partial\boldsymbol{u}^{n}}\right)f\left(\boldsymbol{u}^{n+1}\right)\\
\boldsymbol{b} & =\boldsymbol{A}\boldsymbol{x}
\end{align*}

\end_inset


\end_layout

