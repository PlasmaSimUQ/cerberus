\begin_layout Subsection
Interaction parameter
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\beta= & \frac{p}{\nicefrac{B^{2}}{2\mu_{0}}}=\frac{2\mu_{0}p}{B^{2}}\\
= & \left[\frac{\mu_{0}m_{0}n_{0}u_{0}^{2}}{B_{0}^{2}}\right]\frac{2\hat{p}}{\hat{B}^{2}}\\
= & \left[\frac{\mu_{0}m_{0}n_{0}u_{0}^{2}\beta_{0}}{2\mu_{0}n_{0}m_{0}u_{0}^{2}}\right]\frac{2\hat{p}}{\hat{B}^{2}}\\
= & \left[\frac{\beta_{0}}{2}\right]\frac{2\hat{p}}{\hat{B}^{2}}\\
\beta= & \frac{\beta_{0}\hat{p}}{\hat{B}^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Alfven speed
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
v_{A,x}=\sqrt{\frac{B_{x}^{2}}{\mu_{0}\rho}+\frac{p_{\perp}-p_{\parallel}}{\rho}b_{x}^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\rho=\sum_{\alpha}\rho_{\alpha}$
\end_inset

, 
\begin_inset Formula $p_{\perp}=\sum_{\alpha}p_{\alpha}^{\perp}$
\end_inset

, 
\begin_inset Formula $p_{\parallel}=\sum_{\alpha}p_{\alpha}^{\parallel}$
\end_inset

 and 
\begin_inset Formula $\boldsymbol{b}=\boldsymbol{B}/\left|\boldsymbol{B}\right|$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\beta_{0}\equiv\frac{2\mu_{0}n_{0}m_{0}u_{0}^{2}}{B_{0}^{2}}.
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
v_{A} & =\sqrt{\frac{B^{2}}{\mu_{0}\rho}+\frac{p_{\perp}-p_{\parallel}}{\rho}b^{2}}\\
\left[\frac{u_{0}^{2}}{1}\right]\hat{v}_{A}^{2} & =\left[\frac{B_{0}^{2}}{\mu_{0}\rho_{0}}\right]\frac{\hat{B}^{2}}{\hat{\rho}}+\left[\frac{p_{0}}{\rho_{0}}\right]\frac{\hat{p}_{\perp}-\hat{p}_{\parallel}}{\hat{\rho}}b^{2}\\
\hat{v}_{A}^{2} & =\left[\frac{B_{0}^{2}}{\mu_{0}\rho_{0}u_{0}^{2}}\right]\frac{\hat{B}^{2}}{\hat{\rho}}+\left[\frac{p_{0}}{\rho_{0}u_{0}^{2}}\right]\frac{\hat{p}_{\perp}-\hat{p}_{\parallel}}{\hat{\rho}}b^{2}\\
\hat{v}_{A}^{2} & =\frac{2}{\beta_{0}}\frac{\hat{B}^{2}}{\hat{\rho}}+\frac{\hat{p}_{\perp}-\hat{p}_{\parallel}}{\hat{\rho}}b^{2}\\
\hat{v}_{A}^{2} & =\frac{2\hat{B}^{2}}{\beta_{0}\hat{\rho}}+\frac{b^{2}}{\hat{\rho}}\left(\hat{p}_{\perp}-\hat{p}_{\parallel}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Plasma length scales
\end_layout

\begin_layout Subsubsection
Reference
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
d_{D,0}= & \sqrt{\frac{\epsilon_{0}k_{B}T_{0}}{n_{0}q_{0}^{2}}}=\sqrt{\frac{u_{0}^{2}\epsilon_{0}m_{0}}{n_{0}q_{0}^{2}}}=\sqrt{\frac{u_{0}^{2}m_{0}}{c^{2}\mu_{0}n_{0}q_{0}^{2}}},\\
d_{L,0}= & \frac{u_{0}m_{0}}{q_{0}B_{0}},
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\hat{d}_{D,0}= & \frac{d_{D,0}}{x_{0}}, & \hat{d}_{L,0}= & \frac{d_{L,0}}{x_{0}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\hat{d}_{L,0}}{\hat{d}_{D,0}^{2}} & =\frac{u_{0}m_{0}}{x_{0}q_{0}B_{0}}\frac{x_{0}^{2}n_{0}q_{0}^{2}}{u_{0}^{2}\epsilon_{0}m_{0}}\\
\frac{\hat{d}_{L,0}}{\hat{d}_{D,0}^{2}} & =\frac{x_{0}n_{0}q_{0}}{u_{0}\epsilon_{0}B_{0}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
d_{S,0}\equiv & \frac{1}{q_{0}}\sqrt{\frac{m_{0}}{\mu_{0}n_{0}}}=\frac{u_{0}\hat{c}}{q_{0}}\sqrt{\frac{\epsilon_{0}m_{0}}{n_{0}}}=\hat{c}d_{D,0}, & \beta_{0} & \equiv\frac{2\mu_{0}n_{0}m_{0}u_{0}^{2}}{B_{0}^{2}}=\frac{2}{\hat{c}^{2}}\frac{n_{0}m_{0}}{\epsilon_{0}B_{0}^{2}}=2\frac{d_{L,0}^{2}}{d_{S,0}^{2}}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\sqrt{\frac{\beta_{0}}{2}}\frac{1}{\hat{d}_{S,0}} & =\sqrt{\frac{\mu_{0}n_{0}m_{0}u_{0}^{2}}{B_{0}^{2}}}\frac{x_{0}q_{0}}{1}\sqrt{\frac{\mu_{0}n_{0}}{m_{0}}}\\
 & =\sqrt{\frac{\mu_{0}^{2}n_{0}^{2}u_{0}^{2}x_{0}^{2}q_{0}^{2}}{B_{0}^{2}}}\\
 & =\frac{\mu_{0}n_{0}u_{0}x_{0}q_{0}}{B_{0}}\\
 & =\frac{n_{0}u_{0}x_{0}q_{0}}{c^{2}\epsilon_{0}B_{0}}\\
\sqrt{\frac{\beta_{0}}{2}}\frac{1}{\hat{d}_{S,0}} & =\frac{1}{\hat{c}^{2}}\frac{n_{0}x_{0}q_{0}}{u_{0}\epsilon_{0}B_{0}}\\
\sqrt{\frac{\beta_{0}}{2}}\frac{\hat{c}^{2}}{\hat{d}_{S,0}} & =\frac{n_{0}x_{0}q_{0}}{u_{0}\epsilon_{0}B_{0}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\beta_{0} & =2\frac{d_{L,0}^{2}}{d_{S,0}^{2}}.\\
d_{L,0}^{2} & =d_{S,0}^{2}\frac{\beta_{0}}{2}\\
d_{L,0} & =d_{S,0}\sqrt{\frac{\beta_{0}}{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Larmor
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
d_{L,\alpha}= & \frac{u_{\alpha\perp}m_{\alpha}}{\left|q_{\alpha}\right|B},\\
\left[x_{0}\right]\hat{d}_{L,\alpha}= & \left[\frac{u_{0}m_{0}}{q_{0}B_{0}}\right]\frac{\hat{u}_{\alpha\perp}\hat{m}_{\alpha}}{\left|\hat{q}_{\alpha}\right|\hat{B}},\\
\hat{d}_{L,\alpha}= & \left[\frac{u_{0}m_{0}}{q_{0}B_{0}x_{0}}\right]\frac{\hat{u}_{\alpha\perp}\hat{m}_{\alpha}}{\left|\hat{q}_{\alpha}\right|\hat{B}},\\
\hat{d}_{L,\alpha}= & \hat{d}_{L,0}\frac{\hat{u}_{\alpha\perp}\hat{m}_{\alpha}}{\left|\hat{q}_{\alpha}\right|\hat{B}},\\
\hat{d}_{L,\alpha}= & \hat{c}\hat{d}_{D,0}\frac{\hat{u}_{\alpha\perp}\hat{m}_{\alpha}}{\left|\hat{q}_{\alpha}\right|}\sqrt{\frac{\beta}{2\hat{p}}},
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Debye
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
d_{D,\alpha} & =\sqrt{\frac{\epsilon_{0}k_{B}T_{\alpha}}{n_{\alpha}q_{\alpha}^{2}}}\\
\left[x_{0}^{2}\right]\hat{d}_{D,\alpha}^{2} & =\left[\frac{\epsilon_{0}k_{B}T_{0}}{n_{0}q_{0}^{2}}\right]\frac{\hat{T}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}}\\
\hat{d}_{D,\alpha}^{2} & =\left[\frac{\epsilon_{0}k_{B}T_{0}}{x_{0}^{2}n_{0}q_{0}^{2}}\right]\frac{\hat{T}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}}\\
\hat{d}_{D,\alpha}^{2} & =\hat{d}_{D,0}^{2}\frac{\hat{T}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}}\\
\hat{d}_{D,\alpha} & =\hat{d}_{D,0}\sqrt{\frac{\hat{T}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}}}\\
\hat{d}_{D,\alpha} & =\frac{\hat{d}_{D,0}}{\hat{n}_{\alpha}\hat{q}_{\alpha}}\sqrt{\hat{p}_{\alpha}}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Skin depth
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
d_{S,\alpha} & =\frac{1}{q_{\alpha}}\sqrt{\frac{m_{\alpha}}{\mu_{0}n_{\alpha}}},\\
\left[x_{0}\right]\hat{d}_{S,\alpha} & =\left[\frac{1}{q_{0}}\right]\frac{1}{\hat{q}_{\alpha}}\sqrt{\left[\frac{m_{0}}{\mu_{0}n_{0}}\right]\frac{\hat{m}_{\alpha}}{\hat{n}_{\alpha}}},\\
\hat{d}_{S,\alpha}^{2} & =\left[\frac{m_{0}}{q_{0}^{2}x_{0}^{2}\mu_{0}n_{0}}\right]\frac{\hat{m}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}},\\
\hat{d}_{S,\alpha}^{2} & =\left[\frac{m_{0}\epsilon_{0}c_{0}^{2}}{q_{0}^{2}x_{0}^{2}n_{0}}\right]\frac{\hat{m}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}},\\
\hat{d}_{S,\alpha}^{2} & =\left[\frac{m_{0}\epsilon_{0}u_{0}^{2}}{q_{0}^{2}x_{0}^{2}n_{0}}\right]\frac{\hat{c}^{2}\hat{m}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}},\\
\hat{d}_{S,\alpha}^{2} & =\hat{d}_{D,0}^{2}\frac{\hat{c}^{2}\hat{m}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}},\\
\hat{d}_{S,\alpha}^{2} & =\frac{\hat{d}_{S,0}^{2}\hat{m}_{\alpha}}{\hat{n}_{\alpha}\hat{q}_{\alpha}^{2}},
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Frequencies
\end_layout

\begin_layout Subsubsection
Plasma 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\omega_{p} & =\sqrt{\frac{nq^{2}}{\epsilon_{0}m}}\\
\left[\frac{u_{0}^{2}}{x_{0}^{2}}\right]\hat{\omega}_{p}^{2} & =\left[\frac{n_{0}q_{0}^{2}}{\epsilon_{0}m_{0}}\right]\frac{\hat{n}\hat{q}^{2}}{\hat{m}}\\
\hat{\omega}_{p}^{2} & =\left[\frac{x_{0}^{2}n_{0}q_{0}^{2}}{u_{0}^{2}\epsilon_{0}m_{0}}\right]\frac{\hat{n}\hat{q}^{2}}{\hat{m}}\\
\hat{\omega}_{p}^{2} & =\frac{\hat{n}\hat{q}^{2}}{\hat{d}_{D,0}^{2}\hat{m}}\\
\hat{\omega}_{p}^{2} & =\frac{\hat{c}^{2}\hat{n}\hat{q}^{2}}{\hat{d}_{S,0}^{2}\hat{m}}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Cyclotron
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\omega_{c}= & \frac{qB}{m}\\
\left[\frac{u_{0}}{x_{0}}\right]\hat{\omega}_{c}= & \left[\frac{q_{0}B_{0}}{m_{0}}\right]\frac{\hat{q}\hat{B}}{\hat{m}}\\
\hat{\omega}_{c}= & \left[\frac{x_{0}q_{0}B_{0}}{u_{0}m_{0}}\right]\frac{\hat{q}\hat{B}}{\hat{m}}\\
\hat{\omega}_{c}= & \frac{\hat{q}\hat{B}}{\hat{d}_{L,0}\hat{m}}\\
\hat{\omega}_{c}= & \frac{\hat{q}\hat{B}}{d_{S,0}\hat{m}}\sqrt{\frac{2}{\beta_{0}}}
\end{align*}

\end_inset


\end_layout

